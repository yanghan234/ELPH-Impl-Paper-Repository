&control
    calculation   = 'vc-relax',
    restart_mode  = 'from_scratch',
    prefix        = 'c',
    disk_io       = 'low',
    wf_collect    = .true.
    pseudo_dir    = '~/PseudoPotential/upf',
    outdir        = './',
    forc_conv_thr = 1.0D-3
/
&system
    ibrav         = 2,
    ! unit cell has a = 3.5473, reported in Antonius PRL 2014
    a             = 14.1892,
    nat           = 128,
    ntyp          = 1,
    ecutwfc       = 50.0,
    nbnd          = 300,
    nosym         = .true.,
    noinv         = .true.,
    force_symmorphic = .true.
    input_dft     = 'pbe'
    nspin         = 1
    tot_charge    = 0
    ! tot_magnetization = 0
/
&electrons
    conv_thr        = 1.0e-10,
    diago_full_acc  = .true.,
    mixing_beta     = 0.7,
    diagonalization = 'cg',
    mixing_mode     = 'local-TF'
    electron_maxstep= 500
/
&ions
    ion_dynamics    = "bfgs"
/
&cell
    cell_dynamics   = "bfgs",
    cell_dofree     = "all"
/
